  <POU Name="RpcHandlers" Id="{fbc4ed80-9089-4ee0-90a4-c8ce0b5286d6}" SpecialFunc="None">
    <Declaration><![CDATA[{attribute 'linkalways'} 
{attribute 'no_explicit_call' := 'This FB is a CLASS and must be accessed using methods or properties'}
{attribute 'enable_dynamic_creation'}
FUNCTION_BLOCK RpcHandlers IMPLEMENTS I_RpcHandlers
VAR
	handlers : Dictionary;
END_VAR
]]></Declaration>
    <Implementation>
      <ST><![CDATA[]]></ST>
    </Implementation>
    <Method Name="RegisterHandler" Id="{d3154567-c647-4342-8adf-8dd763b944c1}">
      <Declaration><![CDATA[METHOD PUBLIC RegisterHandler
VAR_INPUT
	MethodName : T_MAXSTRING;
	Handler : I_RpcHandler;
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[handlers.AddOrUpdate(MethodName, Handler);]]></ST>
      </Implementation>
    </Method>
    <Method Name="TryGetByMethodName" Id="{5df45ae3-3bb0-4b1f-bcbc-064f59ae90ed}">
      <Declaration><![CDATA[METHOD PUBLIC TryGetByMethodName : BOOL
VAR_INPUT
	MethodName : T_MAXSTRING;
	Destination : REFERENCE TO I_RpcHandler;
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[TryGetByMethodName := handlers.TryGetValue(MethodName, Destination);]]></ST>
      </Implementation>
    </Method>
  </POU>
